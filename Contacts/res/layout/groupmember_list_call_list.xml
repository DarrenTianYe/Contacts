<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearlayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentLeft="true"
    android:background="#dce3eb"
    android:orientation="vertical"
    android:visibility="visible" >

    <com.android.internal.widget.NeoPrivateCallPanel
        android:id="@+id/neoPrivateCallPanel1"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:callViewStyleType="widget" />

    <ListView
        android:id="@+id/list"
        android:layout_width="fill_parent"
        android:layout_height="330dip"
        android:layout_below="@+id/neoPrivateCallPanel1"
        android:cacheColorHint="#00000000"
        android:divider="#dce3eb"
        android:dividerHeight="2dip"
        android:fadingEdge="none"
        android:listSelector="#00000000"
        android:text="@+string/normalName" >
    </ListView>

    <!--
         <ImageButton
